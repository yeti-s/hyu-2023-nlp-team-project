{
    "name": "NLP Project Chrome Extension",
    "description": "Service that finds an article with a fishing title and changes the title of the article to suit the content.",
    "version": "1.0.0",
    "content_security_policy": {
        "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
    },
    "host_permissions": [
        "https://127.0.0.1:8000/*",
        "http://127.0.0.1:8000/*"
    ],
    "permissions": [
        "scripting",
        "activeTab",
        "contextMenus",
        "tabs",
        "storage"
    ],
    "action": {
        "default_popup": "index.html",
        "default_icon": "icons/Icon-192.png"
    },
    "icons": {
        "128": "icons/Icon-192.png"
    },
    "manifest_version": 3
}